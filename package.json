{
  "name": "coetorise",
  "version": "1.0.0",
  "description": "bus booking system",
  "main": "app.mjs",
  "prettier": {
    "arrowParens": "avoid",
    "useTabs": false,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": false,
    "endOfLine": "lf"
  },
  "scripts": {
    "prisma-generate": "npx prisma generate",
    "dev": "SET NODE_ENV=development && nodemon app.mjs",
    "build": "webpack --stats-error-details --config webpack.config.js",
    "lint": "npx eslint --fix src/**",
    "start": "SET NODE_ENV=production && node ./build/bundle.js"
  },
  "author": "khoi.nd",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "morgan": "^1.10.0",
    "rotating-file-stream": "^3.0.4",
    "socket.io": "^4.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/plugin-transform-runtime": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "babel-loader": "^8.2.5",
    "eslint": "^8.17.0",
    "nodemon": "^2.0.16",
    "prisma": "^3.14.0",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  }
}
